
@model RegisterDTO

@{
    ViewData["Title"] = "تسجيل الدخول";
    var accountType = (AccountType)ViewData["accountType"];
}

<form asp-area="" asp-action="Register" asp-controller="Account" method="post">

    <div class="row">
        <div class="col-md-12">
            <div class="input-group input-group-outline my-3">
                <label class="" asp-for="FirstName"></label>
                <input class="" asp-for="FirstName" />
            </div>
              <div class="input-group input-group-outline my-3">
                <label class="" asp-for="LastName"></label>
                <input class="" asp-for="LastName" />
            </div>
              <div class="input-group input-group-outline my-3">
                <label class="" asp-for="Email"></label>
                <input class="" asp-for="Email" />
            </div>
              <div class="input-group input-group-outline my-3">
                <label class="" asp-for="Password"></label>
                <input class="" asp-for="Password" />
            </div>
        </div>

    </div>

</form>



